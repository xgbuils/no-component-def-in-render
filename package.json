{
	"name": "eslint-restricted-nested-component-def",
	"type": "module",
	"version": "0.1.0",
	"description": "avoid nested component definitions ",
	"main": "src/index.js",
	"scripts": {
		"format": "prettier . --write",
		"lint": "eslint",
		"prepare": "husky install",
		"pre-commit": "npm run format && npm run test",
		"test": "node test/index.js",
		"try": "node test/ruleTester/messageParser.js"
	},
	"keywords": [
		"eslint",
		"no",
