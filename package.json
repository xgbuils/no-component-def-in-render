		"component",
		"definition"
	],
	"author": "Xavier Garcia Buils <xgbuils@gmail.com>",
	"license": "ISC",
	"devDependencies": {
		"chalk": "^5.3.0",
		"diff": "^5.1.0",
		"eslint": "^8.49.0",
		"eslint-config-airbnb": "^19.0.4",
		"glob": "^10.3.10",
		"husky": "^8.0.3",
		"prettier": "^3.0.3"
	}
}
